{
    "name": "pwa-boilerplate",
    "version": "0.0.1",
    "description": "Boilerplate for a modern PWA",
    "main": "index.js",
    "scripts": {
        "dev": "NODE_ENV=development node server/serve.js",
        "production": "NODE_ENV=production node server/serve.js",
        "pack-production": "run-p pack-client-production pack-server-production",
        "pack-client-production": "NODE_ENV=production webpack --config build/webpack.client.config.js --progress --hide-modules",
        "pack-server-production": "NODE_ENV=production webpack --config build/webpack.server.config.js --progress --hide-modules",
        "clean": "rm dist/*"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "private": true,
    "dependencies": {
        "@babel/core": "^7.1.6",
        "@babel/plugin-syntax-dynamic-import": "^7.0.0",
        "@babel/preset-env": "^7.1.6",
        "autoprefixer": "^9.3.1",
        "babel-loader": "^8.0.2",
        "babel-minify-webpack-plugin": "^0.3.1",
        "brotli-webpack-plugin": "^1.0.0",
        "compression-webpack-plugin": "^2.0.0",
        "css-loader": "^1.0.1",
        "express": "^4.16.4",
        "express-static-gzip": "^1.1.3",
        "helmet": "^3.15.0",
        "html-webpack-plugin": "^3.2.0",
        "iltorb": "^2.4.1",
        "less": "^3.9.0",
        "less-loader": "^4.1.0",
        "npm-run-all": "^4.1.5",
        "postcss-loader": "^3.0.0",
        "sw-precache-webpack-plugin": "^0.11.5",
        "vue": "^2.5.17",
        "vue-analytics": "^5.16.1",
        "vue-loader": "^15.4.2",
        "vue-router": "^3.0.2",
        "vue-server-renderer": "^2.5.17",
        "vue-style-loader": "^4.1.2",
        "vue-template-compiler": "^2.5.17",
        "vuex": "^3.0.1",
        "webpack": "^4.26.1",
        "webpack-build-notifier": "^0.1.30",
        "webpack-cli": "^3.1.2",
        "webpack-dev-middleware": "^3.4.0",
        "webpack-hot-middleware": "^2.24.3",
        "webpack-visualizer-plugin": "^0.1.11"
    }
